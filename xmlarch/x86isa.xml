<?xml version="1.0"?>
<Isas>
	<Isa id="16">
		<Name>16</Name>
		<Name>8086</Name>
		<Name>x86_16</Name>
		<Name>x86-16</Name>
		<Description>Pre-386</Description>

		<FormatRef idref="basic16" default="true"/>

		<RegisterRef idref="reg8"/>
		<RegisterRef idref="reg16"/>
		<RegisterRef idref="segreg16"/>

		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="FPU"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="fpu"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="MMX"/>
				<FeatureRef idref="3DNow"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="SSE"/>
				<FeatureRef idref="SSE2"/>
				<FeatureRef idref="SSE3"/>
				<FeatureRef idref="SSSE3"/>
				<FeatureRef idref="SSE41"/>
				<FeatureRef idref="SSE42"/>
				<FeatureRef idref="SSE4a"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
				<RegisterRef idref="xmm"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="AVX"/>
				<FeatureRef idref="FMA"/>
				<FeatureRef idref="AES"/>
				<FeatureRef idref="CLMUL"/>
				<FeatureRef idref="XOP"/>
				<FeatureRef idref="FMA4"/>
				<FeatureRef idref="F16C"/>
				<FeatureRef idref="EPTVPID"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
				<RegisterRef idref="xmm"/>
				<RegisterRef idref="ymm"/>
			</ToReg>
		</FeatureMap>

		<CodeFillRef idref="16" default="true">
			<Name>basic</Name>
		</CodeFillRef>
	</Isa>

	<Isa id="32">
		<Name>x86</Name>
		<Name>32</Name>
		<Description>IA-32 and derivatives</Description>

		<FormatRef idref="basic32" default="true"/>

		<RegisterRef idref="reg8"/>
		<RegisterRef idref="reg16"/>
		<RegisterRef idref="reg32"/>
		<RegisterRef idref="segreg32"/>
		<RegisterRef idref="cr"/>
		<RegisterRef idref="dr"/>
		<RegisterRef idref="tr"/>

		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="FPU"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="fpu"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="MMX"/>
				<FeatureRef idref="3DNow"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="SSE"/>
				<FeatureRef idref="SSE2"/>
				<FeatureRef idref="SSE3"/>
				<FeatureRef idref="SSSE3"/>
				<FeatureRef idref="SSE41"/>
				<FeatureRef idref="SSE42"/>
				<FeatureRef idref="SSE4a"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
				<RegisterRef idref="xmm"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="AVX"/>
				<FeatureRef idref="FMA"/>
				<FeatureRef idref="AES"/>
				<FeatureRef idref="CLMUL"/>
				<FeatureRef idref="XOP"/>
				<FeatureRef idref="FMA4"/>
				<FeatureRef idref="F16C"/>
				<FeatureRef idref="EPTVPID"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
				<RegisterRef idref="xmm"/>
				<RegisterRef idref="ymm"/>
			</ToReg>
		</FeatureMap>

		<CodeFillRef idref="basic" default="true">
			<Name>basic</Name>
		</CodeFillRef>
		<CodeFillRef idref="gasbasic">
			<Name>gas</Name>
		</CodeFillRef>
		<CodeFillRef idref="intel">
			<Name>intel</Name>
		</CodeFillRef>
		<CodeFillRef idref="amd">
			<Name>amd</Name>
		</CodeFillRef>
	</Isa>

	<Isa id="64">
		<Name>amd64</Name>
		<Name>intel64</Name>
		<Name>x86_64</Name>
		<Name>x86-64</Name>
		<Name>64</Name>
		<Description>AMD64/Intel64</Description>

		<FormatRef idref="basic64" default="true"/>

		<RegisterRef idref="reg8x"/>
		<RegisterRef idref="reg16x"/>
		<RegisterRef idref="reg32x"/>
		<RegisterRef idref="reg64"/>
		<RegisterRef idref="segreg32"/>
		<RegisterRef idref="cr64"/>
		<RegisterRef idref="dr"/>
		<RegisterRef idref="tr"/>
		<RegisterRef idref="rip"/>

		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="FPU"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="fpu"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="MMX"/>
				<FeatureRef idref="3DNow"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="SSE"/>
				<FeatureRef idref="SSE2"/>
				<FeatureRef idref="SSE3"/>
				<FeatureRef idref="SSSE3"/>
				<FeatureRef idref="SSE41"/>
				<FeatureRef idref="SSE42"/>
				<FeatureRef idref="SSE4a"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
				<RegisterRef idref="xmm64"/>
			</ToReg>
		</FeatureMap>
		<FeatureMap>
			<FromFeatures>
				<FeatureRef idref="AVX"/>
				<FeatureRef idref="FMA"/>
				<FeatureRef idref="AES"/>
				<FeatureRef idref="CLMUL"/>
				<FeatureRef idref="XOP"/>
				<FeatureRef idref="FMA4"/>
				<FeatureRef idref="F16C"/>
				<FeatureRef idref="EPTVPID"/>
			</FromFeatures>
			<ToReg>
				<RegisterRef idref="mmx"/>
				<RegisterRef idref="xmm64"/>
				<RegisterRef idref="ymm64"/>
			</ToReg>
		</FeatureMap>

		<!-- Default to Intel ones if unspecified (to match GAS behavior). -->
		<CodeFillRef idref="intel" default="true">
			<Name>basic</Name>
			<Name>intel</Name>
		</CodeFillRef>
		<CodeFillRef idref="amd">
			<Name>amd</Name>
		</CodeFillRef>
	</Isa>
</Isas>
